@model List<Osoby>
@{
    ViewBag.Title = "ListaOsob2";
}

<h2><b>Lista Osób Wysukanych</b></h2>
<body>
    @foreach (var a in Model)
    {




        <p>
            @a.IdOsoby, @a.Imie, @a.Nazwisko, @a.Wiek @Html.ActionLink("Detale", "Pokaz", a);



        </p>
    }



    <p>@Html.ActionLink("Do pobierz", "Index")</p>
    <p>@Html.ActionLink("Do Usun", "Usun")</p>
    <p>@Html.ActionLink("Do Wyszukaj", "Wyszukaj")</p>
</body>

